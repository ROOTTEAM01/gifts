<!-- product qty + - -->
<script type="text/javascript">
  
$('.add').click(function () {   
  var th = $(this).closest('.wrap').find('.count');     
  th.val(+th.val() + 1);
});
$('.sub').click(function () {
  var th = $(this).closest('.wrap').find('.count');     
      if (th.val() > 1) th.val(+th.val() - 1);
});
</script>
